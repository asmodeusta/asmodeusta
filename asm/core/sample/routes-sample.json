[
  {
    "module-default": {
      "name": "module",
      "match": 0,
      "value": "{module-default}"
    },
    "module-custom": {
      "name": "module",
      "match": "([a-z][a-zA-Z\\_-]{2,63})",
      "value": "$1"
    },
    "controller-default": {
      "name": "controller",
      "match": 0,
      "value": "{controller-default}"
    },
    "controller-custom": {
      "name": "controller",
      "match": "([a-z][a-zA-Z\\_-]{2,63})",
      "value": "$1"
    },
    "action-default": {
      "name": "action",
      "match": 0,
      "value": "{action-default}"
    },
    "action-custom": {
      "name": "action",
      "match": "([a-z][a-zA-Z\\_-]{2,63})",
      "value": "$1"
    },
    "param-short": {
      "name": "{param-name}",
      "match": "([a-z][a-zA-Z\\_-]{2,63})",
      "value": "$1"
    },
    "param-long": {
      "name": 0,
      "match": "{param-name}",
      "value": 0,
      "nodes": [
        {
          "name": "{param-name}",
          "match": "([a-z][a-zA-Z\\_-]{2,63})",
          "value": "$1"
        }
      ]
    }
  }
]